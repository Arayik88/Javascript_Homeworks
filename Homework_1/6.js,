function myFunc(num1, num2, num3) {

   var min = num1;
   if(min > num2){
   	min = num2;
   }
   if(min > num3){
   	min = num3;
   }

   var max = num1;
   if(max < num2){
   	max = num2;
   }
   if(max < num3){
   	max = num3;
   }

    var mid = (num1 + num2 + num3) - (min + max);

    alert(min + " " + mid + " " + max);
}

myFunc(42,7,5);